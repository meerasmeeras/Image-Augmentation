{
    "builds":[{
        "src" :  "image_augmentation_project/wsgi.py",
        "use" :  "@vercel/python",
        "config" :  {"maxLambdaSize":"15mb", "runtime":"python3.12"}
    }
    ],
    "routes":[{
        "src" :  "/(.*)",
        "dest" :  "image_augmentation_project/wsgi.py"
    }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}
